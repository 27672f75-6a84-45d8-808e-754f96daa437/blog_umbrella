<div>
  <.form let={f} for={@changeset}
          phx-change="validate_check"
          phx-target={@myself}
          phx-submit="save" >
    
    <%= label f, :title %>
    <%= text_input f, :title %>
    <%= error_tag f, :title %>
     
    <%= label f, :description %>
    <%= text_input f, :description %>
    <%= error_tag f, :description %>

    <%= submit "Write" %>
  </.form>
</div>